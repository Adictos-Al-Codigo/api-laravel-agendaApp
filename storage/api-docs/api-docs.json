                "tags": [
                    "User"
                ],
                "summary": "Edit user profile",
                "description": "Endpoint to edit user profile information.",
                "operationId": "editProfile",
                "requestBody": {
                    "description": "Profile update data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "firstName",
                                    "lastName"
                                ],
                                "properties": {
