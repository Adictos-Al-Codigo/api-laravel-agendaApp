    },
    "components": {
        "schemas": {
            "AuthLoginRequest": {
                "title": "Auth Login Request",
                "description": "Request body for user login",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "User's email address",
                        "type": "string",
                        "format": "email",
                        "example": "user@example.com"
                    },
                    "password": {
                        "description": "User's password",
                        "type": "string",
                        "example": "password123"
                    }
                },
                "type": "object"
            },
            "AuthRegisterRequest": {
                "title": "Auth Register Request",
                "required": [
                    "firstname",
                    "lastname",
                    "email",
                    "password"
                ],
                "properties": {
                    "firstname": {
                        "description": "First name of the user",
                        "type": "string",
                        "example": "John"
                    },
                    "lastname": {
                        "description": "Last name of the user",
                        "type": "string",
                        "example": "Doe"
                    },
                    "email": {
                        "description": "Email address of the user",
                        "type": "string",
                        "format": "email",
                        "example": "john.doe@example.com"
                    },
                    "password": {
                        "description": "Password for the account",
                        "type": "string",
                        "example": "secret123"
                    }
                },
                "type": "object"
            },
            "ContactResource": {
                "title": "Contact Resource",
                "description": "Contact resource representation",
                "properties": {
                    "id": {
                        "description": "Id of the contact (encrypted)",
                        "type": "integer",
                        "example": "1"
                    },
                    "name": {
                        "description": "Name of the contact",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "phone": {
                        "description": "Phone number of the contact",
                        "type": "string",
                        "example": "123456789"
                    },
                    "nickname": {
                        "description": "Nickname of the contact",
                        "type": "string",
                        "example": "Johnny"
                    }
                },
                "type": "object"
            },
            "UserResource": {
                "title": "User Resource",
                "description": "User resource representation",
                "properties": {
                    "id": {
                        "description": "ID of the user",
                        "type": "integer",
                        "example": "1"
                    },
                    "firstname": {
                        "description": "First name of the user",
                        "type": "string",
                        "example": "John"
                    },
                    "lastname": {
                        "description": "Last name of the user",
                        "type": "string",
                        "example": "Doe"
                    },
                    "email": {
                        "description": "Email address of the user",
                        "type": "string",
                        "format": "email",
                        "example": "john.doe@example.com"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "Token de autenticación",
                "name": "BearerToken",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}