    "components": {
        "schemas": {
            "Contact": {
                "title": "Contact",
                "description": "Contact model",
                "required": [
                    "id",
                    "name",
                    "phone"
                ],
                "properties": {
                    "id": {
                        "description": "Contact ID",
                        "type": "integer",
                        "example": "1"
                    },
                    "name": {
                        "description": "Contact name",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "phone": {
                        "description": "Contact phone number",
                        "type": "string",
                        "example": "123456789"
                    }
                },
                "type": "object"
            },
            "UserResource": {
                "title": "User Resource",
                "description": "User resource representation",
                "properties": {
                    "id": {
                        "description": "ID of the user",
                        "type": "integer",
                        "example": "1"
                    },
                    "firstname": {
                        "description": "First name of the user",
                        "type": "string",
                        "example": "John"
                    },
                    "lastname": {
                        "description": "Last name of the user",
                        "type": "string",
                        "example": "Doe"
                    },
                    "email": {
                        "description": "Email address of the user",
                        "type": "string",
                        "format": "email",
                        "example": "john.doe@example.com"
                    }
                },
                "type": "object"
